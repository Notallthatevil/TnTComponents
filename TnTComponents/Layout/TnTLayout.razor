
<CascadingValue IsFixed="true" Value="_context">
    @if (_context.SideNav is not null) {
        @_context.SideNav
    }
    @if (_context.Header is not null) {
        @_context.Header
    }

    @_context.Body

    @ChildContent
</CascadingValue>
