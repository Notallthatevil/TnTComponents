@typeparam TGridItem
<tbody>
    @{
        var items = Context.Items;

        @if (items.Any()) {
            @foreach (var item in Context.Items) {
                <TnTDataGridBodyRow Item="@item" />
            }
        }
        else {
            <TnTDataGridBodyRowEmpty TGridItem="@(TGridItem)" />
        }
    }
</tbody>