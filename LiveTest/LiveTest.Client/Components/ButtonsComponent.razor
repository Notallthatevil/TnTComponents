@using TnTComponents
<TnTRow>
    <TnTColumn S="12">

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Filled">
            Filled
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Outlined" TextColor="TnTColor.Primary">
            Outlined
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Text" TextColor="TnTColor.Primary">
            Text
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Elevation="0">
            Elevation 0
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Elevation="2">
            Elevation 2
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Elevation="5">
            Elevation 5
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Elevation="10">
            Elevation 10
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" BorderRadius="new TnTComponents.Core.TnTBorderRadius(0)">
            CornerRadius 0
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" BorderRadius="new TnTComponents.Core.TnTBorderRadius(10)">
            CornerRadius 10
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Disabled="true">
            Disabled
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <MaterialIcon Icon="@MaterialIcon.Abc" />
            StartIcon
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <MaterialIcon Icon="@MaterialIcon.Abc" />
            Both
            <MaterialIcon Icon="@MaterialIcon.ZoomOutMap" />
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            EndIcon
            <MaterialIcon Icon="@MaterialIcon.ZoomOutMap" />
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" />
            Progress Ring
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" />
            Progress Linear
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" Value="50" />
            Progress Ring 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" Value="50" />
            Progress Linear 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Outlined" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" />
            Outline With Progress Ring
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Outlined" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" />
            Outline With  Progress Linear
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Outlined" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" Value="50" />
            Outline With Progress Ring 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Outlined" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" Value="50" />
            Outline With Progress Linear 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Text" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" />
            Text With Progress Ring
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Text" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" />
            Text With Progress Linear
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Text" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Ring" Value="50" />
            Text With Progress Ring 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" Appearance="ButtonAppearance.Text" TextColor="TnTColor.Primary">
            <TnTProgressIndicator Appearance="ProgressAppearance.Linear" Value="50" />
            Text With Progress Linear 50%
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" TextAlignment="TextAlign.Left">
            Left
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" TextAlignment="TextAlign.Right">
            Right
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" BackgroundColor="TnTColor.SecondaryContainer" TextColor="TnTColor.OnSecondaryContainer" TintColor="TnTColor.Secondary">
            Colored
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick" BorderRadius="@(new TnTComponents.Core.TnTBorderRadius() {StartStart = 5, StartEnd = 2, EndEnd = 9, EndStart=3})">
            Obscure Radius
        </TnTButton>

        <TnTButton OnClickCallback="@OnClick">
            With Badge
            <TnTComponents.TnTBadge>
                5
            </TnTComponents.TnTBadge>
        </TnTButton>
        
        <TnTButton OnClickCallback="@OnClick">
            With Long Badge
            <TnTComponents.TnTBadge>
                This is a really long badge
            </TnTComponents.TnTBadge>
        </TnTButton>
    </TnTColumn>
</TnTRow>


@code {
    private void OnClick() {
        Console.WriteLine("Button Clicked");
    }
}