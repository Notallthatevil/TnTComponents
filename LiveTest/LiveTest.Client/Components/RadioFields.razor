@using static LiveTest.Client.Pages.Forms

<TnTRadioGroup SupportingText="Supporting Text" Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup SupportingText="Supporting Text" Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton StartIcon="search" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton EndIcon="error" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton StartIcon="search" EndIcon="error" Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    <TnTRadioButton Disabled="true" StartIcon="search" EndIcon="error" Label="1" Value="1" />
    <TnTRadioButton StartIcon="search" EndIcon="error" Label="2" Value="2" />
    <TnTRadioButton Disabled="true" StartIcon="search" EndIcon="error" Label="3" Value="3" />
</TnTRadioGroup>

<TnTRadioGroup SupportingText="Supporting Text" Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup SupportingText="Supporting Text" Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton StartIcon="search" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton EndIcon="error" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    @foreach (var item in Items) {
        <TnTRadioButton StartIcon="search" EndIcon="error" Label="@item.ToString()" Value="@item" />
    }
</TnTRadioGroup>

<TnTRadioGroup Label="RadioGroup" Orientation="TnTComponents.Enum.Orientation.Horizontal" TInputType="int?" @bind-Value="@Model.RadioOption" Name="RadioGroup">
    <TnTRadioButton Disabled="true" StartIcon="search" EndIcon="error" Label="1" Value="1" />
    <TnTRadioButton StartIcon="search" EndIcon="error" Label="2" Value="2" />
    <TnTRadioButton Disabled="true" StartIcon="search" EndIcon="error" Label="3" Value="3" />
</TnTRadioGroup>


@code {
    [CascadingParameter]
    public InputClass Model { get; set; }


    private List<int> Items { get; set; } = new List<int>() {
        1,2,3
    };
}
