@using System.ComponentModel.DataAnnotations
@using TnTComponents
@rendermode InteractiveAuto
<TnTForm Model="@_model" Appearance="FormAppearance.Outlined">
    <DataAnnotationsValidator />

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch @bind-Value="@_model.C" />
    </TnTLabel>

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch StartIcon="MaterialIcon.Gif" @bind-Value="@_model.C" />
    </TnTLabel>

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch @bind-Value="@_model.C" EndIcon="MaterialIcon.Group"/>
    </TnTLabel>

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch StartIcon="MaterialIcon.Gif" @bind-Value="@_model.C" EndIcon="MaterialIcon.Group" />
    </TnTLabel>

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputText  @bind-Value="@_model.S"   />
    </TnTLabel>
</TnTForm>

<TnTForm Model="@_model" Appearance="FormAppearance.Filled">
    <DataAnnotationsValidator />

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch @bind-Value="@_model.C" />
    </TnTLabel>

    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputSwitch StartIcon="MaterialIcon.Gif" @bind-Value="@_model.C" />
