    <TnTLabel Label="My Label" BackgroundColor="TnTColor.Surface" Appearance="FormAppearance.Filled">
        <TnTInputCheckbox StartIcon="MaterialIcon.Gif" @bind-Value="@_model.C" EndIcon="MaterialIcon.Group" />
    </TnTLabel>

</TnTForm>


@(_model.A)


@code {

    private Model _model = new();

    class Model {
        public Test? A { get; set; }
        public string S{ get; set; }
        public bool C{ get; set; }
        public Test? D{ get; set; }
    }

    enum Test {
        One,
        Two,
        Three,
        Four
    }

}